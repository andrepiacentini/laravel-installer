{
    "name": "mauri870/laravel-installer",
    "description": "Install laravel with any version you want.",
    "keywords": ["laravel","installer","custom","version"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mauri de Souza Nunes",
            "email": "mauri870@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mauri870\\LaravelInstaller\\Console\\": "src/",
            "Mauri870\\LaravelInstaller\\Tests\\": "tests/"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "~4.0|~5.0|~6.0",
        "symfony/console": "~2.3|~3.0",
        "symfony/process": "~2.3|~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "satooshi/php-coveralls": "^1.0"
    },
    "bin": [
        "laravel-installer"
    ]
}
